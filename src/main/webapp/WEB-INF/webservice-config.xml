<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://cxf.apache.org/jaxws
       http://cxf.apache.org/schemas/jaxws.xsd">

    <jaxws:endpoint
            id="userEndpoint"
            implementor="ru.stoliarenkoas.tm.webserver.webservices.soap.UserEndpoint"
            address="/UserService"/>
    <jaxws:endpoint
            id="projectEndpoint"
            implementor="ru.stoliarenkoas.tm.webserver.webservices.soap.ProjectEndpoint"
            address="/ProjectService"/>
    <jaxws:endpoint
            id="taskEndpoint"
            implementor="ru.stoliarenkoas.tm.webserver.webservices.soap.TaskEndpoint"
            address="/TaskService"/>

</beans>